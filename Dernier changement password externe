Get-ADUser -Filter * -Properties SamAccountName,UserPrincipalName,PasswordLastSet |
Where-Object { $_.UserPrincipalName -notlike "*@entreprise.local" } |
Select-Object SamAccountName,UserPrincipalName,PasswordLastSet |
Export-Csv -Path "C:\Temp\PasswordLastSet_Externes.csv" -NoTypeInformation -Encoding UTF8
